{
  "providers": {
    "postgresql": {
      "driver": "org.postgresql.Driver",
      "mappings": {
        "varchar": "string",
        "int4": "integer",
        "int2": "integer",
        "date": "date",
        "float4": "double",
        "real": "double",
        "bool": "boolean",
        "serial": "integer",
        "default": "string"
      }
    }
  },
  "databases": {
    "default": {
      "provider": "postgresql",
      "host": "localhost",
      "port": "5432",
      "database": "test_db"
    }
  },
  "features": {
    "newSearchFeature": {
      "enabled": true
    },
    "notificationsFeature": {
      "enabled": false
    }
  }
}